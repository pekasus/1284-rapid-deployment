const int key1 = 20;
const int key2 = 21;
const int key3 = 18;
const int key4 = 19;
const int led1 = 24;
const int led2 = 25;
const int led3 = 26;
const int led4 = 27;

int keyPins[] = { key1, key2, key3, key4 };
int keyPinCount = 4;
int ledPins[] = { led1, led2, led3, led4 };
int ledPinCount = 4;

void setup() {

  pinMode(key1, INPUT_PULLUP);
  pinMode(key2, INPUT_PULLUP);
  pinMode(key3, INPUT_PULLUP);
  pinMode(key4, INPUT_PULLUP);
  pinMode(led1, OUPUT);
  pinMode(led2, OUPUT);
  pinMode(led3, OUPUT);
  pinMode(led4, OUPUT);
